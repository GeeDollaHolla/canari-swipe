{
	"author": "Grant Lonie",
	"name": "canari-swipe",
	"version": "2.1.6",
	"description": "Swiping library designed for Canari platform",
	"homepage": "https://github.com/GeeDollaHolla/canari-swipe#readme",
	"license": "MIT",
	"main": "index.js",
	"files": [
		"index.js"
	],
	"scripts": {
		"start": "parcel ./src/index.html",
		"test": "jest --watch",
		"build": "babel ./src/Swiper.js --out-file index.js --presets=@babel/preset-env,@babel/preset-react && parcel build ./src/index.js --out-dir docs"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		],
		"plugins": [
			"transform-object-rest-spread",
			"transform-class-properties"
		]
	},
	"jest": {
		"testURL": "http://localhost",
		"setupFiles": [
			"<rootDir>/src/tests/setupTests.js"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/src/tests/__mocks__/styleMock.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeeDollaHolla/canari-swipe.git"
	},
	"keywords": [
		"react",
		"swiper",
		"swipe",
		"canari"
	],
	"peerDependencies": {
		"react": "^16.4.1",
		"react-dom": "^16.4.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.1.2",
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"@babel/preset-react": "^7.0.0",
		"babel-cli": "^6.26.0",
		"babel-jest": "^23.6.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"enzyme": "^3.7.0",
		"enzyme-adapter-react-16": "^1.6.0",
		"enzyme-to-json": "^3.3.4",
		"jest": "^23.6.0",
		"parcel-bundler": "^1.10.3",
		"react": "^16.6.0",
		"react-dom": "^16.6.0",
		"react-test-renderer": "^16.6.0"
	},
	"dependencies": {
		"prop-types": "^15.6.2"
	}
}
